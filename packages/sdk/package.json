{
  "name": "@somethingsexy/sdk",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "author": "Tyler J Cvetan",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "swc ./src -d dist --strip-leading-paths",
    "build:watch": "swc ./src -d dist -w  --strip-leading-paths",
    "lint": "eslint ./src --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint  ./src --report-unused-disable-directives --max-warnings 0 --fix",
    "pretty:fix": "npx prettier . --write  --ignore-unknown",
    "release": "npx semantic-release",
    "test": "echo \"Error: no test specified\" && exit 0",
    "type-check": "tsc --noEmit"
  },
  "release": {
    "branches": [
      "main"
    ],
    "extends": "semantic-release-monorepo"
  },
  "dependencies": {
    "@mantine/core": "^7.10.1",
    "@mantine/hooks": "^7.10.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.5.28",
    "prettier": "3.3.2",
    "typescript": "^5.4.5"
  }
}
